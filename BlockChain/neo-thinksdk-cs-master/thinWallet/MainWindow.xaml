<Window x:Class="thinWallet.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:thinWallet"
        mc:Ignorable="d"
        Title="neo-ThinSdk-test" Height="350" Width="525" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="thinwallet/back1.jpg"/>
        </Grid.Background>
        <Grid.RowDefinitions>
            <RowDefinition Height="50*"/>
            <RowDefinition Height="50*"/>
        </Grid.RowDefinitions>
        <Button Content="Fuction Test" HorizontalAlignment="Center" Margin="0" VerticalAlignment="Center" Width="220" Height="64" Click="Button_Click"/>
        <Button Content="ThinWallet Test" HorizontalAlignment="Center" Margin="0" VerticalAlignment="Center" Width="220" Height="64" Grid.Row="1" Click="Button_Click_1"/>

    </Grid>
</Window>
