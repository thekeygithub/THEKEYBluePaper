        {
          "id": "regurl",
          "type": "url",
          "desc": "input a url wantto get.",
          "value": "youwelcome.test"
        }
      ],
      "call": {
        "type": "sendrawtransaction",
        "scriptcall": "(uint160)%consts.registerContract%",
        "scriptparam": [
          "(str)requestSubDomain",
          [
            "(bytes)%keyinfo.scripthash%",
